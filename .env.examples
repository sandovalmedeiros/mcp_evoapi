# Exemplo de configuração de variáveis de ambiente para o evoapi_mcp

# Dados da EVO API
EVO_API_TOKEN=SEU_TOKEN_EVO_API
EVO_INSTANCE_NAME=NOME_DA_INSTANCIA
EVO_INSTANCE_TOKEN=TOKEN_DA_INSTANCIA
EVO_BASE_URL=http://localhost:8081

# APIs externas (opcionais)
DEEPSEEK_API_KEY=sua_chave_deepseek
KLUSTER_AI_API_KEY=sua_chave_kluster
OPENAI_API_KEY=sua_chave_openai
SERPER_API_KEY=sua_chave_serper
GROQ_API_KEY=sua_chave_groq
TAVILY_API_KEY=sua_chave_tavily

# Acesso a dados externos (exemplo)
BRIGHDATA_USERNAME=seu_email
BRIGHDATA_PASSWORD=sua_senha
SERP_API=sua_chave_serp

# Conexão com o banco de dados
DATABASE_URL=postgresql+psycopg2://usuario:senha@localhost:5432/nome_do_banco

